package com.internousdev.myecsite01.action;

import java.sql.SQLException;
import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.internousdev.myecsite01.dao.CreateCompleteDAO;
import com.opensymphony.xwork2.ActionSupport;

public class CreateCompleteAction extends ActionSupport implements SessionAware{

	private String loginUserId;
	private String loginPassword;
	private String userName;
	public Map<String, Object> session;
	private CreateCompleteDAO ccDAO = new CreateCompleteDAO();

	public String execute() throws SQLException{
		ccDAO.createUser(session.get("loginUserId").toString(),
				session.get("loginPasswird").toString(),
				session.get("userName").toString());

		String result = SUCCESS;
		return result;
	}

	public String getLoginUserId(){
		return loginUserId;
	}

	public void setLoginUserId(String loginUserId){
		this.loginUserId = loginUserId;
	}

}
